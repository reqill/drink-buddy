generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:Admin0@localhost:5432/postgers" //TODO: env("DATABASE_URL")
}

model Drink {
  id  String @id @default(uuid())
  name String
}